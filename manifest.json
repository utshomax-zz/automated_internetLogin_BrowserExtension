{
    "name": "GitHub login extension",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"]
        
    }],
    "permissions": [
        "tabs"
      ],
    "background":{
        "scripts":["background.js"]
    }
  
}
