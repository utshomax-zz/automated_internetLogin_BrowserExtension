{
    "name": "AIL NITR",
    "version": "2.0.3",
    "manifest_version": 2,
    "content_scripts": [{
        "matches": ["*://192.168.1.220/*",
            "*://*.google.com/*"],
        "js": ["content.js"]
        
    }],
    "permissions": [
        "tabs",
        "storage"
      ],
    "background":{
        "scripts":["background.js"]
    },
    "browser_action":{
        "default_icon":"icons/icon512.png",
        "default_popup":"pop/index.html",
        "default_title":"AIL NITR"
    },
    "icons": { "16": "icons/icon16.png",
        "32": "icons/icon32.png",
       "128": "icons/icon128.png" }
}
