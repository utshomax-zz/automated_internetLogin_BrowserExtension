{
    "name": "AIL NITR",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [{
        "matches": ["*://192.168.1.220/*",
            "*://*.google.com/*"],
        "js": ["content.js"]
        
    }],
    "permissions": [
        "tabs",
        "storage",
        "*://*.google.com/*"
      ],
    "background":{
        "scripts":["background.js"]
    },
    "browser_action":{
        "default_icon":"b_icon.png",
        "default_popup":"pop/index.html",
        "default_title":"AIL NITR"
    }
  
}
